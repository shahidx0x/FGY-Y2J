{
  "apps": [
    {
      "name": "FGI-Y2J",
      "script": "src/index.js",
      "instances": "max",
      "exec_mode": "cluster",
      "autorestart": true,
      "watch": false,
      "time": true,
      "error_file": "./logs/err.log",
      "out_file": "./logs/out.log",
      "exp_backoff_restart_delay": 101, 
      "env_production": {
        "NODE_ENV": "production",
        "MAINTENANCE_MODE": "false"
      },
      "env_maintenance": {
        "NODE_ENV": "maintenance",
        "MAINTENANCE_MODE": "true"
      }
    }
  ]
}
